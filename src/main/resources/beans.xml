<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="admin" class="com.kiosk.mckiosk.model.User">
        <constructor-arg value="admin"/>
        <constructor-arg value="admin"/>
        <constructor-arg>
            <value type="com.kiosk.mckiosk.model.AccountType">ADMIN</value>
        </constructor-arg>
    </bean>

    <bean id="user" class="com.kiosk.mckiosk.model.User">
        <constructor-arg value="user"/>
        <constructor-arg value="user"/>
        <constructor-arg>
            <value type="com.kiosk.mckiosk.model.AccountType">USER</value>
        </constructor-arg>
    </bean>


<!--    WSTAWIE TU POSIÅKI-->
</beans>
